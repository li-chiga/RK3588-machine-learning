&i2c5 {
	dsi0_touch: gt911@14 {
		compatible = "goodix,gt9xx";
		reg = <0x14>;
		pinctrl-names = "default";
		pinctrl-0 = <&touch0_gpio>;
		touch-gpio = <&gpio3 RK_PC0 IRQ_TYPE_LEVEL_LOW>;
		reset-gpio = <&gpio3 RK_PC1 GPIO_ACTIVE_HIGH>;
		max-x = <1080>;
		max-y = <1920>;
		tp-size = <911>;
		tp-supply = <&vcc5v0_sys>;
		wakeup-source;
		goodix-ts-name = "dsi0_ts_gt9xx";
		vendor-id = <0xABCD>;
		product-id = <0x1234>;
		status = "disabled";
	};
};

&i2c6 {
	dsi1_touch: gt911@14 {
		compatible = "goodix,gt9xx";
		reg = <0x14>;
		pinctrl-names = "default";
