//&mipi_dcphy0 {
//	status = "disabled";
//};

&dsi0_panel {
	power-supply = <&vcc5v0_sys>;
	reset-gpios = <&gpio4 RK_PA3 GPIO_ACTIVE_LOW>;
	pinctrl-names = "default";
	pinctrl-0 = <&lcd0_rst_gpio>;
	reset-delay-ms = <100>;
	init-delay-ms = <80>;

	panel-exit-sequence = [
		05 32 01 28
		05 C8 01 10
	];
	status = "disabled";
};

