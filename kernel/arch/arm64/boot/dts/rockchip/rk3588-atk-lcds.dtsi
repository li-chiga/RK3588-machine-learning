		touch-gpio = <&gpio3 RK_PD5 IRQ_TYPE_LEVEL_LOW>;
		reset-gpio = <&gpio3 RK_PA6 GPIO_ACTIVE_HIGH>;
		max-x = <1080>;
		max-y = <1920>;
		tp-size = <911>;
		tp-supply = <&vcc5v0_sys>;
		wakeup-source;
		goodix-ts-name = "dsi1_ts_gt9xx";
